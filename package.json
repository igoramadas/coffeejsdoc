{
    "name": "coffeejsdoc",
    "description": "A documentation generator for JavaScript and CoffeeScript",
    "version": "1.0.0",
    "keywords": ["documentation", "doc", "jsdoc", "coffeescript"],
    "bugs": {
        "url": "https://github.com/igoramadas/coffeejsdoc/issues"
    },
    "author": "Sangmin Yoon <sangmin.yoon@croquis.com>",
    "license": "MIT",
    "bin": {
        "coffeejsdoc": "./bin/coffeejsdoc"
    },
    "scripts": {
        "build": "coffee -c -o lib src",
        "doc": "node bin/coffeejsdoc",
        "test": "NODE_ENV=test mocha --compilers coffee:coffeescript/register"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/igoramadas/coffeejsdoc.git"
    },
    "engines": {
        "node": ">= 0.10"
    },
    "dependencies": {
        "coffeescript": "^2.0.3",
        "fs.extra": "^1.3.2",
        "glob": "^7.1.2",
        "inflect": "^0.4.0",
        "pug": "^2.0.0-rc.4",
        "js-yaml": "^3.10.0",
        "jsdoctypeparser": "^2.0.0-alpha-8",
        "lodash": "^4.17.4",
        "marked": "^0.3.7",
        "optparse": "^1.0.5",
        "walkdir": "^0.0.12"
    },
    "devDependencies": {
        "chai": "^4.1.2",
        "coffee-coverage": "^2.0.1",
        "crojsdoc-plugin-auto-namespace": "^0.1.1",
        "crojsdoc-plugin-underscore-private": "^0.1.0",
        "mocha": "^4.0.1"
    }
}
