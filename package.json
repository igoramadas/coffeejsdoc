{
    "name": "coffeejsdoc",
    "version": "0.9.4",
    "description": "A documentation generator for JavaScript and CoffeeScript.",
    "keywords": [
        "documentation",
        "doc",
        "jsdoc",
        "coffeescript"
    ],
    "author": "Igor Ramadas <igor@devv.com>",
    "homepage": "https://coffeejsdoc.devv.com",
    "subdomain": "coffeejsdoc",
    "license": "MIT",
    "main": "index",
    "repository": {
        "type": "git",
        "url": "https://github.com/igoramadas/coffeejsdoc.git"
    },
    "engines": {
        "node": ">= 0.10"
    },
    "bin": {
        "coffeejsdoc": "./bin/coffeejsdoc"
    },
    "scripts": {
        "build": "coffee -c -b -o lib src",
        "docs": "node bin/coffeejsdoc",
        "test": "NODE_ENV=test mocha --compilers coffee:coffeescript/register"
    },
    "dependencies": {
        "coffeescript": "^2.3.2",
        "crojsdoc-plugin-auto-namespace": "^0.1.1",
        "crojsdoc-plugin-underscore-private": "^0.1.0",
        "fs.extra": "^1.3.2",
        "glob": "^7.1.3",
        "inflect": "^0.4.0",
        "pug": "^2.0.3",
        "js-yaml": "^3.12.0",
        "jsdoctypeparser": "^2.0.0-alpha-8",
        "lodash": "^4.17.11",
        "marked": "^0.5.2",
        "optparse": "^1.0.5",
        "walkdir": "^0.0.12"
    },
    "devDependencies": {
        "chai": "^4.2.0",
        "coffee-coverage": "^3.0.1",
        "mocha": "^5.2.0"
    }
}
